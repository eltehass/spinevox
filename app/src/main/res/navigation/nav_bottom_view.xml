<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_bottom_view"
    app:startDestination="@id/navigation_tab1">

    <fragment
        android:id="@+id/navigation_tab1"
        android:name="com.leo.spinevox.screens.diagnostic.DiagnosticMainMenuFragment"
        android:label="DiagnosticMainMenuFragment" >
        <action
            android:id="@+id/action_navigation_tab1_to_navigation_tab2"
            app:destination="@id/navigation_tab2" />
    </fragment>

    <fragment
        android:id="@+id/navigation_tab2"
        android:name="com.leo.spinevox.screens.diagnostic.DiagnosticMainMenuFragment"
        android:label="DiagnosticMainMenuFragment" >
        <action
            android:id="@+id/action_navigation_tab2_to_navigation_tab3"
            app:destination="@id/navigation_tab3" />
    </fragment>

    <fragment
        android:id="@+id/navigation_tab3"
        android:name="com.leo.spinevox.screens.diagnostic.DiagnosticMainMenuFragment"
        android:label="DiagnosticMainMenuFragment" >
        <action
            android:id="@+id/action_navigation_tab3_to_navigation_tab4"
            app:destination="@id/navigation_tab4" />
    </fragment>


    <fragment
        android:id="@+id/navigation_tab4"
        android:name="com.leo.spinevox.screens.diagnostic.DiagnosticMainMenuFragment"
        android:label="DiagnosticMainMenuFragment" />

</navigation>